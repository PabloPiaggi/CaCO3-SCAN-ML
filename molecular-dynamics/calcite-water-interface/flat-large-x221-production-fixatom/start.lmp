echo both

include		in.temp
include		in.pressure
include		in.seed
units		metal
atom_style	atomic
include		in.box
include 	in.setup

include         in.dump

timer           timeout 105:55:00 every 1000

group 		fixCa id 8220 2952 5580 312

fix             2 all nph z ${pressure} ${pressure} ${pressureDamp}
fix             3 all temp/csvr ${temperature} ${temperature} ${tempDamp} ${seed}
fix             4 all momentum 10000 linear 1 1 1 rescale
fix 		5 fixCa spring/self 10.0

run             20000000 # 10. ns

write_data	data.final

write_restart   restart.lmp
